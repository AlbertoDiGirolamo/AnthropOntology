                  owl:equivalentClass AnthropOntology:Habitat ;
                  rdfs:subClassOf obo:ENVO_01001110 ,
                                  AnthropOntology:Habitat ;
                  obo:IAO_0000115 "An environmental system which can sustain and allow the growth of an ecological population." ;
                  oboInOwl:hasDbXref "EcoLexicon:habitat" ,
                                     "LTER:238" ,
                                     "SWEETRealm:Habitat" ,
                                     "https://en.wikipedia.org/wiki/Habitat" ;
